{
  "name": "schwab-momentum",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "npm --prefix web i && npm --prefix server i || true",
    "build": "npm --prefix web run build",
    "start": "node server/index.js",
    "dev": "concurrently \"npm:dev:web\" \"npm:dev:server\"",
    "dev:web": "npm --prefix web run dev",
    "dev:server": "NODE_ENV=development nodemon server/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "vite": "^5.3.4",
    "@vitejs/plugin-react": "^4.3.1"
  }
}

